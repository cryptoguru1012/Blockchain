{
  "name": "shopshots",
  "version": "1.0.0",
  "description": "Syscoin marketplace",
  "main": "server.js",
  "scripts": {
    "preinstall": "npm run lint",
    "build": "NODE_ENV='production' webpack -p",
    "start": "NODE_ENV='production' node server",
    "dev": "nodemon ./server.js set NODE_ENV='development'",
    "front:dev": "webpack -w --color --progress",
    "wds": "webpack-dev-server --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --port 8050 --host 0.0.0.0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cdev": "concurrently \"npm run dev\" \"npm run front:dev\" \"npm run wds\"",
    "lint": "eslint .",
    "lint:log": "eslint . > eslint.log",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
